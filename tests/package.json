{
  "name": "md-roam-server-tests",
  "version": "1.0.0",
  "description": "E2E tests for md-roam-server API",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:japanese": "vitest run e2e/japanese-unicode.test.ts",
    "test:errors": "vitest run e2e/error-handling.test.ts",
    "test:performance": "vitest run e2e/performance.test.ts",
    "test:workflows": "vitest run e2e/workflows.test.ts",
    "test:core": "vitest run e2e/nodes.test.ts e2e/search.test.ts e2e/files.test.ts e2e/server.test.ts e2e/metadata.test.ts",
    "test:extended": "vitest run e2e/japanese-unicode.test.ts e2e/error-handling.test.ts e2e/performance.test.ts e2e/workflows.test.ts",
    "test:all": "vitest run --reporter=verbose",
    "test:quick": "vitest run --reporter=basic --bail=1",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "vite": "^5.0.10",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}